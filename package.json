{
  "name": "issue-repo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:ts": "tsc --build",
    "dev:ts": "tsc --build --watch",
    "doc": "yarn typedoc --name \"Legend Studio\" --hideGenerator --gitRemote upstream --gitRevision master",
    "repro": "yarn build:ts && yarn doc"
  },
  "devDependencies": {
    "typedoc": "0.23.3",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.0"
}
